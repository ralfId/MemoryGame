    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="MemoryGame.Views.UI.GameScoresTabs.Level3Page"
    xmlns:vm="clr-namespace:MemoryGame.ViewModels"
    x:DataType="vm:GameScoresPageVM"
    Style="{x:StaticResource NavPageStyle}">
    <ContentPage.Content>
                <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
            <Label Text="Level 3 Page" FontSize="Title" FontAttributes="Bold"/>
            <Label Text="{Binding Name}" FontSize="Title" FontAttributes="Bold"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>

